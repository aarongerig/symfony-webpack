<div class="tm-header tm-header-transparent" data-uk-header>
    <div data-uk-sticky="sel-target: .uk-navbar-container; cls-active: uk-navbar-sticky;
                     cls-inactive: uk-navbar-transparent uk-light; animation: uk-animation-slide-top;
                     top: .tm-header ~ [class*='uk-section'], .tm-header ~ * > [class*='uk-section']">
        <div class="uk-navbar-container uk-navbar-transparent uk-light">
            <div class="uk-container">
                <nav data-uk-navbar="align: center; boundary: !.uk-navbar-container">

                    <div class="uk-navbar-left">

                        <a href="#hero" class="uk-navbar-item uk-logo" data-uk-scroll>
                            <img width="200" src="{{ asset('build/images/logo.svg') }}" class="uk-responsive-height" alt="Vision">
                            <img width="200" src="{{ asset('build/images/logo-inverted.svg') }}" class="uk-responsive-height uk-logo-inverse" alt="Vision">
                        </a>

                    </div>

                    <div class="uk-navbar-right">

                        <div class="uk-visible@s">
                            <ul class="uk-navbar-nav" data-uk-scrollspy-nav="closest: li; scroll: true">
                                {% include 'navigation/navbar.html.twig' %}
                            </ul>
                        </div>

                        <div class="uk-hidden@s">
                            <a class="uk-navbar-toggle" href="#off-canvas" data-uk-toggle>
                                <span data-uk-navbar-toggle-icon></span>
                            </a>

                            {% include 'navigation/offcanvas.html.twig' %}
                        </div>

                    </div>

                </nav>
            </div>
        </div>
    </div>
</div>